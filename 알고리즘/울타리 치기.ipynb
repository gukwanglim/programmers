    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "dp = [0, 1]\n",
    "for n in range(2, N+1):\n",
    "    dp.append(dp[-1] + (n // 6) - (n % 6 == 1) + 1)\n",
    "print(dp[N])"
   ]
