    "def solution(k, d):\n",
    "    answer = 0\n",
    "    \n",
    "    for i in range(0, d+1, k):\n",
    "        answer += (d ** 2 - i ** 2) ** (1/2) // k + 1\n",
    "    \n",
    "    return answer"
   ]
