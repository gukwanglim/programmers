    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "M = str(input()).strip()[::-1]\n",
    "\n",
    "answer = 0\n",
    "for i in range(len(M)):\n",
    "    answer += (int(M[i]) - (int(M[i]) > 4)) * pow(9, i)\n",
    "print(answer)"
