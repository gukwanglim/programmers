   "source": [
    "# 문자열 안의 숫자는 str.isdigit()으로 찾는다\n",
    "\n",
    "def solution(my_string):\n",
    "    answer = 0\n",
    "    \n",
    "    list_str = list(map(str, my_string))\n",
    "    \n",
    "    for i in list_str:\n",
    "        if str.isdigit(i) == True:\n",
    "            answer += int(i)\n",
    "    \n",
    "    return answer"
   ]
  }
